    <select id="getUserPackages" resultType="long">
        SELECT tracking_id
        FROM user_tracking_tb
        WHERE user_id=#{userID};
    </select>
    <insert id="insertTracking">
        INSERT INTO user_tracking_tb (user_id,tracking_id)
        values (#{userID},#{trackingID});
    </insert>

    
